{
  "name": "hinasila/dicontainer",
  "description": "PSR-11 compliant PHP dependency injection container",
  "keywords": [
    "psr-11",
    "dependency-injection",
    "container",
    "container-interop",
    "di"
  ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Asis Pattisahusiwa",
      "email": "asis.pattisahusiwa@gmail.com"
    }
  ],
  "homepage": "https://github.com/hinasila/dicontainer",
  "support": {
    "issues": "https://github.com/hinasila/dicontainer/issues"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Hinasila\\DiContainer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    },
    "classmap": [
      "tests/_fixtures"
    ]
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": false,
      "forward-command": true
    }
  }
}
