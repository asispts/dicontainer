{
    "name": "xynha/dicontainer",
    "description": "PSR-11 compliant PHP dependency injection container",
    "keywords": [
        "psr-11",
        "dependency-injection",
        "container",
        "container-interop",
        "di"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Asis Pattisahusiwa",
            "email": "asis.pattisahusiwa@gmail.com"
        }
    ],
    "homepage": "https://github.com/pattisahusiwa/dicontainer",
    "support": {
        "issues": "https://github.com/pattisahusiwa/dicontainer/issues"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.0"
        }
    },
    "autoload": {
        "psr-4": {
            "Xynha\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xynha\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "pattisahusiwa/coding-standard": "^0.3.0",
        "phpunit/phpunit": "^6.5"
    }
}
